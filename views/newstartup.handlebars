
<div class="container">
  <div class="page-header text-center">
    <h1>Add A New Startup</h1>
  </div>

  <form method="POST" style="width: 400px; margin: 0px auto">
    <div class="form-group">
      <label>Name: </label>
      <input type="text" class="form-control" placeholder="Pied Piper"
        name="name">
    </div>

    <div class="form-group">
      <label>Logo: </label>
      <input type="text" class="form-control" placeholder="logo.png"
        name="logo">
    </div>

    <div class="form-group">
      <label>Email: </label>
      <input type="email" class="form-control"
        placeholder="bachmanity@pieppiper.com" name="email">
    </div>

    <div class="form-group">
      <label>Phone: </label>
      <input type="phone" class="form-control" placeholder="555-555-5555"
        name="phone">
    </div>

    <div class="form-group">
      <label>Location: </label>
      <input type="text" class="form-control" placeholder="505 Central Ave NW"
        name="location">
    </div>

    <div class="form-group">
      <label>Facebook: </label>
      <input type="text" class="form-control"
        placeholder="https://facebook.com/piedpiper" name="facebook">
    </div>

    <div class="form-group">
      <label>Twitter: </label>
      <input type="text" class="form-control"
        placeholder="https://twitter.com/PiedPiper" name="twitter">
    </div>

    <div class="form-group">
      <label>Instagram: </label>
      <input type="text" class="form-control"
        placeholder="https://instagam.com/piedpiper" name="instagram">
    </div>

    <div class="form-group">
      <label>Angel List: </label>
      <input type="text" class="form-control"
        placeholder="https://angel.co/piedpiper" name="angellist">
    </div>

    <button type="submit" class="btn btn-primary">Send</button>
  </form>
</div>
